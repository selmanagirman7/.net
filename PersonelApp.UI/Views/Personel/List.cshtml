@model List<PersonelApp.Model.Personel>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Ad Soyad</th>
        <th>Departman</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.AdSoyad</td>
            <td>@item.Departman.Ad</td>
            <td>
            </td>
        </tr>
    }
</table>
